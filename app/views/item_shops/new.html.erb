<%= form_with url: '/item_shops', method: :post, local: true do |form| %>
  <%= form.label :name %>
  <%= form.text_field :name %>
  <%= form.label :city %>
  <%= form.text_field :city %>
  <%= form.label :open %>
  <%= form.text_field :open %>
  <%= form.label :num_employees %>
  <%= form.text_field :num_employees %>
  <%= form.submit "Create Item Shop" %>
<% end %>