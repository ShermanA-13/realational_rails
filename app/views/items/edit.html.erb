<%= form_with url: "/items/#{@item.id}", method: :patch, local: true do |form| %>
  <%= form.label :name %>
  <%= form.text_field :name %>
  <%= form.label :quantity %>
  <%= form.text_field :quantity %>
  <%= form.label :price %>
  <%= form.text_field :price %>
  <%= form.label :consumable %>
  <%= form.text_field :consumable %>
  <%= form.submit "Update Item" %>
<% end %>