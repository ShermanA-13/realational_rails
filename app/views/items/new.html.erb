<%= form_with url: "/item_shops/#{@item_shop.id}/items", method: :post, local: true do |form| %>
  <%= form.label :name %>
  <%= form.text_field :name %></br>
  <%= form.label :quantity %>
  <%= form.text_field :quantity %></br>
  <%= form.label :price %>
  <%= form.text_field :price %></br>
  <%= form.label :consumable %>
  <%= form.text_field :consumable %></br>
  <%= form.submit "Generate" %>
<% end %>